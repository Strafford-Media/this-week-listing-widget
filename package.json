{
  "name": "external-app-template",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "MODE=development parcel serve src/index.html -p 2023",
    "build": "MODE=production parcel build src/index.tsx --no-scope-hoist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tehpsalmist/external-app-template.git"
  },
  "keywords": [
    "parcel",
    "react",
    "tailwindcss",
    "duda",
    "external app",
    "template"
  ],
  "author": "Ben Steward <bstewardcodes@gmail.com> (https://tehpsalmist.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehpsalmist/external-app-template/issues"
  },
  "homepage": "https://github.com/Strafford-Media/external-app-template#readme",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.10.1",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "parcel": "^2.10.1",
    "process": "^0.11.10",
    "tailwindcss": "^3.3.5"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "app": "public/index.js",
  "targets": {
    "main": false,
    "app": {
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
