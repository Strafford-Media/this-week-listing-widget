{
  "name": "this-week-listing-widget",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "MODE=development parcel serve src/index.html -p 2023",
    "build": "MODE=production parcel build",
    "watch": "NODE_ENV=production parcel watch -p 2023",
    "test": "node -r ts-node/register --loader ts-node/esm --test ./tests/pg_trgm.spec.ts",
    "test:watch": "node --watch -r ts-node/register --loader ts-node/esm --test ./tests/pg_trgm.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tehpsalmist/this-week-listing-widget.git"
  },
  "keywords": [
    "parcel",
    "preact",
    "tailwindcss",
    "duda",
    "external app",
    "template"
  ],
  "author": "Ben Steward <bstewardcodes@gmail.com> (https://tehpsalmist.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehpsalmist/this-week-listing-widget/issues"
  },
  "homepage": "https://github.com/Strafford-Media/this-week-listing-widget#readme",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.10.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.8.9",
    "autoprefixer": "^10.4.16",
    "parcel": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "process": "^0.11.10",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1"
  },
  "full-listing": "public/full-listing.js",
  "listing-list": "public/listing-list.js",
  "featured-listings": "public/featured-listings.js",
  "search-bar": "public/search-bar.js",
  "ad-placement": "public/ad-placement.js",
  "ad-group": "public/ad-group.js",
  "evvnt-gallery": "public/evvnt-gallery.js",
  "hawaiian-islands": "public/hawaiian-islands.js",
  "this-week-widgets-css": "public/this-week-widgets.css",
  "targets": {
    "main": false,
    "full-listing": {
      "source": "src/full-listing.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "listing-list": {
      "source": "src/listing-list.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "featured-listings": {
      "source": "src/featured-listings.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "search-bar": {
      "source": "src/search-bar.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "ad-placement": {
      "source": "src/ad-placement.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "ad-group": {
      "source": "src/ad-group.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "evvnt-gallery": {
      "source": "src/evvnt-gallery.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "hawaiian-islands": {
      "source": "src/hawaiian-islands.tsx",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    },
    "this-week-widgets-css": {
      "source": "src/tailwind.css",
      "scopeHoist": false,
      "engines": {
        "browsers": "IE 11"
      },
      "outputFormat": "global"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.3",
    "preact": "^10.19.3"
  },
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime",
    "react": "preact/compat",
    "react-dom": "preact/compat"
  }
}
